<!DOCTYPE html>
{% include './blocks/head.twig' %}

<body>
    {% include './blocks/preloader.twig' %}
    <div id="wrapper">
        <div class="page-wrapper">
            {% include './blocks/navbar.twig' %}
            <!-- main banner -->
            <section class="banner banner-inner parallax" data-stellar-background-ratio="0.5" id="list-view-normal">
                <div class="banner-text">
                    <div class="text-center">
                        <div class="container">
                            <h1>Rentals and Activities</h1>
                            <strong class="subtitle">Winter</strong>
                        </div>
                    </div>
                </div>
            </section>

            <!-- main container -->
            <main id="main">
                <aside class="recent-block recent-list recent-wide-thumbnail">
                    <div class="container">
                        <h2 class="text-center text-uppercase">Winter (est. October - April)</h2>
                        <div class="row">
                            <article class="col-sm-6 col-md-3 article">
                                <div class="thumbnail">
                                    <h3 class="no-space"><a href="/fishing">Aurora Hunt</a></h3>
                                    <strong class="info-title">Activity</strong>
                                    <div class="img-wrap">
                                        <img src="img/listing/img-31.jpg" height="210" width="250"
                                            alt="image description">
                                    </div>
                                    <footer>
                                        <div class="sub-info" style="width:100%;">
                                            <span style="float:left;">per day</span>
                                            <span style="float:right;">€ 0</span>

                                        </div>
                                    </footer>
                                </div>
                            </article>
                            <article class="col-sm-6 col-md-3 article">
                                <div class="thumbnail">
                                    <h3 class="no-space"><a href="/familyigloo">Suzuki Samurai</a></h3>
                                    <strong class="info-title">Rental</strong>
                                    <div class="img-wrap">
                                        <img src="img/listing/img-31.jpg" height="210" width="250"
                                            alt="image description">
                                    </div>
                                    <footer>
                                        <div class="sub-info" style="width:100%;">
                                            <span style="float:left;">per day</span>
                                            <span style="float:right;">€ 0</span>

                                        </div>
                                    </footer>
                                </div>
                            </article>
                            <article class="col-sm-6 col-md-3 article">
                                <div class="thumbnail">
                                    <h3 class="no-space"><a href="/familyigloo">Daihatsu Feroza</a></h3>
                                    <strong class="info-title">Rental</strong>
                                    <div class="img-wrap">
                                        <img src="img/listing/img-31.jpg" height="210" width="250"
                                            alt="image description">
                                    </div>
                                    <footer>
                                        <div class="sub-info" style="width:100%;">
                                            <span style="float:left;">per day</span>
                                            <span style="float:right;">€ 0</span>

                                        </div>
                                    </footer>
                                </div>
                            </article>
                            <article class="col-sm-6 col-md-3 article">
                                <div class="thumbnail">
                                    <h3 class="no-space"><a href="/tipitent">Voyager Safari</a></h3>
                                    <strong class="info-title">Activity</strong>
                                    <div class="img-wrap">
                                        <img src="img/listing/img-31.jpg" height="210" width="250"
                                            alt="image description">
                                    </div>
                                    <footer>
                                        <div class="sub-info" style="width:100%;">
                                            <span style="float:left;">per 4 hours</span>
                                            <span style="float:right;">€ 0</span>

                                        </div>
                                    </footer>
                                </div>
                            </article>
                        </div>
                    </div>
                </aside>
            </main>
        </div>
        {% include './blocks/footer.twig' %}
    </div>
    {% include './blocks/scripts.twig' %}
</body>

</html>